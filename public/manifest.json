{
  "name": "Flip Book",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Save content to flip book",
  "permissions": ["contextMenus"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
